{
  "name": "gandi-ext",
  "version": "1.0.0",
  "description": "Next-generation Gandi Extension.",
  "main": "dist/main.js",
  "scripts": {
    "lint": "eslint ./src",
    "fix": "eslint ./src --fix",
    "server": "webpack serve --mode development",
    "build": "npm run clean && webpack --config webpack.config.js --mode=production",
    "devbuild": "npm run clean && webpack --config webpack.config.js --mode=development",
    "watch": "npm run clean && webpack --watch --config webpack.config.js --progress --mode=production",
    "clean": "rimraf ./dist && rimraf types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FurryR/not.js.git"
  },
  "author": "FurryR",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FurryR/not.js/issues"
  },
  "homepage": "https://github.com/FurryR/not.js#readme",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@types/node": "^18.14.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "uglify-js": "^3.17.4",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  }
}
